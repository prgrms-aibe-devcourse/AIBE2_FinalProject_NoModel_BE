{
  "id": null,
  "uid": "method-logging",
  "title": "Method-Level Logging Dashboard",
  "tags": ["loki", "method-logging", "nomodel"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 0,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Method Execution Overview",
      "type": "stat",
      "targets": [
        {
          "expr": "count by (level) (count_over_time({application=\"nomodel\"} | json | level != \"\" [5m]))",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "displayMode": "list",
            "orientation": "horizontal"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "red", "value": 80}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Method Execution Times",
      "type": "timeseries",
      "targets": [
        {
          "expr": "{application=\"nomodel\"} | json | execution_time_ms != \"\" | unwrap execution_time_ms",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {"legend": false, "tooltip": false, "vis": false},
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {"type": "linear"},
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {"group": "A", "mode": "none"},
            "thresholdsStyle": {"mode": "off"}
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "red", "value": 80}
            ]
          },
          "unit": "ms"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 3,
      "title": "Method-Level Error Logs",
      "type": "logs",
      "targets": [
        {
          "expr": "{application=\"nomodel\"} | json | level=\"ERROR\" | line_format \"{{.full_method_name}} - {{.error_message}}\"",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "gridPos": {"h": 9, "w": 24, "x": 0, "y": 8},
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": false,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      }
    },
    {
      "id": 4,
      "title": "Controller vs Service Method Performance",
      "type": "barchart",
      "targets": [
        {
          "expr": "avg by (layer) (avg_over_time({application=\"nomodel\"} | json | execution_time_ms != \"\" | unwrap execution_time_ms [5m]))",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "",
            "axisPlacement": "auto",
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": {"legend": false, "tooltip": false, "vis": false},
            "lineWidth": 1,
            "orientation": "auto",
            "stacking": "none"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "red", "value": 80}
            ]
          },
          "unit": "ms"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 17}
    },
    {
      "id": 5,
      "title": "Top Slow Methods",
      "type": "table",
      "targets": [
        {
          "expr": "topk(10, avg by (full_method_name) (avg_over_time({application=\"nomodel\"} | json | execution_time_ms != \"\" | unwrap execution_time_ms [5m])))",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "custom": {"align": "auto", "displayMode": "auto"},
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 500},
              {"color": "red", "value": 1000}
            ]
          },
          "unit": "ms"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 17}
    },
    {
      "id": 6,
      "title": "Method Success/Error Rate",
      "type": "piechart",
      "targets": [
        {
          "expr": "count by (status) (count_over_time({application=\"nomodel\"} | json | status != \"\" [5m]))",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {"hideFrom": {"legend": false, "tooltip": false, "vis": false}},
          "mappings": []
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 25},
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
        "pieType": "pie",
        "tooltip": {"mode": "single", "sort": "none"},
        "legend": {"displayMode": "visible", "placement": "bottom"}
      }
    },
    {
      "id": 7,
      "title": "HTTP Method Distribution",
      "type": "piechart",
      "targets": [
        {
          "expr": "count by (http_method) (count_over_time({application=\"nomodel\"} | json | http_method != \"\" [5m]))",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {"hideFrom": {"legend": false, "tooltip": false, "vis": false}},
          "mappings": []
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 25},
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
        "pieType": "pie",
        "tooltip": {"mode": "single", "sort": "none"},
        "legend": {"displayMode": "visible", "placement": "bottom"}
      }
    }
  ],
  "schemaVersion": 30,
  "version": 1,
  "links": []
}